{
    "name": "@aws/mynah-ui",
    "displayName": "AWS Mynah UI",
    "version": "4.38.0",
    "description": "AWS Toolkit VSCode and Intellij IDE Extension Mynah UI",
    "private": true,
    "main": "./dist/main.js",
    "types": "./dist/main.d.ts",
    "scripts": {
        "compile": "tsc --build",
        "build": "webpack --config webpack.config.js --mode production",
        "lint": "eslint src --ext .ts,.tsx",
        "test": "jest",
        "test:unit": "jest --collect-coverage",
        "test:coverage": "jest --collect-coverage",
        "test:e2e:local": "cd ui-tests && npx playwright test",
        "test:e2e:docker": "npm run docker:clean && npm run docker:build && npm run docker:run",
        "docker:clean": "docker rm -f mynah-ui-e2e-container || true",
        "docker:build": "docker build -t mynah-ui-e2e .",
        "docker:run": "docker run --name mynah-ui-e2e-container -e WEBKIT_FORCE_COMPLEX_TEXT=0 -e WEBKIT_DISABLE_COMPOSITING_MODE=1 mynah-ui-e2e",
        "docker:extract": "docker cp mynah-ui-e2e-container:/app/ui-tests/__results__ ./ui-tests/ && docker cp mynah-ui-e2e-container:/app/ui-tests/__snapshots__ ./ui-tests/__results__/__snapshots__"
    },
    "dependencies": {
        "escape-html": "^1.0.3",
        "highlight.js": "^11.11.0",
        "just-clone": "^6.2.0",
        "marked": "^14.1.0",
        "sanitize-html": "^2.12.1",
        "unescape-html": "^1.1.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/node": "^22.9.0",
        "@types/sanitize-html": "^2.11.0",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "babel-jest": "^29.7.0",
        "core-js": "^3.33.3",
        "css-loader": "6.6.0",
        "eslint": "^8.42.0",
        "eslint-plugin-unused-imports": "^4.1.4",
        "fast-check": "^3.22.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-svg-transformer": "^1.0.0",
        "sass": "1.49.8",
        "sass-loader": "12.6.0",
        "style-loader": "3.3.1",
        "svg-url-loader": "^8.0.0",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "typescript": "^5.8.2",
        "webpack": "5.94.0",
        "webpack-cli": "4.7.2"
    }
}
