{
    "all": true,
    "check-coverage": false,
    "reporter": ["text", "html", "lcov"],
    "reports-dir": "coverage",
    "include": ["out/**/*.js"],
    "exclude": ["out/**/*.test.js", "out/**/*.spec.js", "out/**/test/**"],
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
}
